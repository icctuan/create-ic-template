{
	"name": "react-webpack",
	"description": "react + webpack + typescript",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"start": "webpack serve --config webpack.dev.cjs",
		"build": "webpack --config webpack.prod.js",
		"prepare": "husky install",
		"lint-staged": "lint-staged"
	},
	"lint-staged": {
		"**/*.css": "stylelint --fix **/*.css",
